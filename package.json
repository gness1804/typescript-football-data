{
  "name": "@typescript/playground",
  "version": "1.0.0",
  "description": "Football data.",
  "author": "",
  "private": true,
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.12",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "^7.4.0",
